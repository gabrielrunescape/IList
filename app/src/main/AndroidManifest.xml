<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ilist.gabrielrunescape.com.br">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="ilist.gabrielrunescape.com.br.view.HomeActivity" android:label="@string/app_name">
            <intent-filter android:label="@string/app_name">
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
                <category android:name="android.intent.category.APP_CALENDAR" />
                <category android:name="android.intent.category.APP_CALCULATOR" />
                <data android:scheme="http" android:host="gabrielrunescape.ddns.net" android:pathPrefix="/iList" />
            </intent-filter>
        </activity>

        <activity
            android:name=".view.ItemActivity"
            android:label="@string/activity_item"
            android:parentActivityName=".view.HomeActivity">
        </activity>
    </application>

</manifest>